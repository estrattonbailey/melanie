{
  "name": "",
  "version": "",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parallelshell 'npm run files:dev' 'npm run js:dev' 'npm run css:dev'",
    "files:render": "node render.js",
    "files:dev": "onchange 'src/markup/**/*.js' -- npm run files:render",
    "js:build": "browserify src/js/index.js --debug -p [minifyify --map index.js.map --output index.js.map] > site/index.js",
    "js:dev": "watchify src/js/index.js -v -d -o site/index.js",
    "css:build": "node-sass src/css/main.scss | postcss -u autoprefixer > site/style.css",
    "css:dev": "onchange 'src/**/*.scss' -- npm run css:build"
  },
  "browserify": {
    "debug": true,
    "cache": {},
    "packageCache": {},
    "extension": [
      "js"
    ],
    "transform": [
      "babelify"
    ]
  },
  "browserify-shim": {},
  "repository": {
    "type": "git",
    "url": "git+ssh://"
  },
  "author": "estrattonbailey",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "autoprefixer": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babelify": "*",
    "browserify": "*",
    "exorcist": "*",
    "fab.js": "*",
    "minifyify": "*",
    "node-sass": "^4.5.0",
    "onchange": "*",
    "parallelshell": "*",
    "postcss-cli": "*",
    "react": "^15.3.2",
    "react-markdown": "^2.4.5",
    "watchify": "*"
  },
  "dependencies": {
    "fab.js": "0.0.4",
    "h0": "^1.0.0-beta.4",
    "layzr.js": "^2.2.1",
    "loop.js": "^0.2.2",
    "operator.js": "^2.0.1",
    "putz": "0.0.2",
    "tarry.js": "^0.4.1"
  }
}
