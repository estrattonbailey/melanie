{
  "name": "",
  "version": "",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run files:build && npm run css:build ; npm run copy:images && concurrently 'npm run data:dev' 'npm run files:dev' 'npm run js:dev' 'npm run css:dev'",
    "build": "npm run files:build && npm run js:build && npm run css:build",
    "serve": "cd site/ && live-server",
    "copy:images": "mkdir -p site/images && cp -r images/ site/images/",
    "data:dev": "onchange 'data.js' -- npm run files:build",
    "files:build": "node render.js",
    "files:dev": "onchange 'src/markup/**/*.js' -- npm run files:build",
    "js:build": "browserify src/js/index.js --debug -p [minifyify --map index.js.map --output index.js.map] > site/index.js",
    "js:dev": "watchify src/js/index.js -v -d -o site/index.js",
    "css:build": "node-sass src/css/main.scss | postcss -u autoprefixer > site/style.css",
    "css:dev": "onchange 'src/**/*.scss' -- npm run css:build"
  },
  "browserify": {
    "debug": true,
    "cache": {},
    "packageCache": {},
    "extension": [
      "js"
    ],
    "transform": [
      "babelify"
    ]
  },
  "browserify-shim": {},
  "repository": {
    "type": "git",
    "url": "git+ssh://"
  },
  "author": "estrattonbailey",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "autoprefixer": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babelify": "^8.0.0",
    "browserify": "*",
    "exorcist": "*",
    "fab.js": "^0.3.6",
    "live-server": "^1.2.1",
    "minifyify": "*",
    "node-sass": "^4.13.1",
    "onchange": "*",
    "postcss-cli": "*",
    "react": "^15.3.2",
    "react-markdown": "^2.4.5",
    "watchify": "*"
  },
  "dependencies": {
    "bricks.js": "^1.8.0",
    "concurrently": "^4.1.0",
    "h0": "^1.0.0-beta.4",
    "layzr.js": "^2.2.1",
    "loop.js": "^0.2.2",
    "operator.js": "^2.0.1",
    "putz": "0.0.2",
    "remark": "^8.0.0",
    "remark-react": "^4.0.1",
    "tarry.js": "^0.4.1"
  }
}
